{
	"name": "shopware-console",
	"displayName": "Shopware Console Run Most Important Shopware Commands",
	"description": "Run most important Shopware commands from VsCode UI without writing any line.",
	"publisher": "andreasmueck-vscode",
	"icon": "images/icon.png",
	"author": {
		"name": "Andreas Mueck"
	},
	"homepage": "https://github.com/AndreasMueck/shopware-console/blob/master/README.md",
	"license": "SEE LICENSE IN LICENSE",
	"repository": {
		"type": "git",
		"url": "https://github.com/AndreasMueck/shopware-console"
	},
	"bugs": "https://github.com/AndreasMueck/shopware-console/issues",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:shopware.cacheClear",
		"onCommand:shopware.displayAbout"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "shopware.cacheClear",
				"title": "cache:clear",
				"category": "Shopware"
			},
			{
				"command": "shopware.displayAbout",
				"title": "about",
				"category": "Shopware"
			}
		],
		"configuration": {
			"title": "superConsole",
			"properties": {
				"superConsole.path": {
					"type": "string",
					"default": "php bin/console",
					"description": "Set another path to console"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.67.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	},
	"extensionPack": [
		"andreasmueck.shopware-code-snippets"
	]
}